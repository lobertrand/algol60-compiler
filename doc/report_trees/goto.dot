digraph {

	ordering=out;
	ranksep=.4;
  margin=0;
	bgcolor="white"; node [shape=box, fixedsize=false, fontsize=12, fontname="Helvetica-bold", fontcolor="black"
		width=.25, height=.25, color="#444444", fillcolor="white", style="filled, solid"];
	edge [arrowsize=.5, color="#444444"]

  n4 [label="GOTO"];
  n5 [label="myLabel"];

  n4 -> n5 // "GOTO" -> "myLabel"

}
